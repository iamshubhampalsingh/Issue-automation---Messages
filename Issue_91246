import uiautomator2 as ui2
import time

# Device serial number
device_serial = "192.168.2.138:5555"

# Create a Device object for the device
receiver_device = ui2.Device(device_serial)

receiver_device.app_start("com.google.android.contacts")
time.sleep(2)
if receiver_device.xpath('//*[@resource-id="com.google.android.contacts:id/contacts"]/android.widget.FrameLayout[1]/android.widget.ImageView[1]').exists:
    print("We are in contact interface")
else:
    print("We are not in contact interface")
receiver_device(resourceId="com.android.systemui:id/back").click()
